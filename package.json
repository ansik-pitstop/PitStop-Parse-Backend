{
  "name": "pitstop-parse-push-server",
  "version": "1.0.0",
  "description": "push server based on parse server",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ansik-pitstop/PitStop-Parse-Backend"
  },
  "license": "MIT",
  "dependencies": {
    "config": "^1.24.0",
    "express": "^4.12.3",
    "forever": "^0.15.3",
    "parse-server": "^1.8.5",
    "winston": "^2.3.0"
  },
  "scripts": {
    "start-staging": "ENVIRONMENT=staging ./node_modules/.bin/forever start ./forever/staging.json",
    "stop-staging": "./node_modules/.bin/forever stop parse-server-staging || exit 0",
    "start-production": "ENVIRONMENT=production ./node_modules/.bin/forever start ./forever/production.json",
    "stop-production": "./node_modules/.bin/forever stop parse-server-production || exit 0"
  }
}
